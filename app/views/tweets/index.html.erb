<p>Welcome! </p>

<ul>
<% @tweets.each do |tweet| %>
  <li>
    User: <%= tweet.user.email %>
    Tweet: <%= tweet.text %>

    <% if current_user.id == tweet.user.id %>
      <%= link_to "Edit", edit_tweet_path(tweet) %>
    <% end %>

  </li>
  <% end %>
</ul>

<%= link_to "New Tweet", new_tweet_path %>
