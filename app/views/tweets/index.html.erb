<div class="container">
  <div class="row">
    <div class="col-md-6 col-md-offset-3 well">
      <h1>Try Tweeting!</h1>
      <div id='new-tweet-form-container'>
        <%= render partial: 'tweetform' %>
      </div>

    </div>
  </div>
</div>


<div class="container">
  <div class="row">
    <div id="tweet-list" class="col-md-6 col-md-offset-3 well">
      <%= render partial: 'tweetlist' %>
    </div>
  </div>
</div>

<div>
  <h2>Registered Users:</h2>
  <% @users.each do |user| %>
    <%= link_to user.email, profile_path(user) %>
    <br/>
  <% end %>
</div>
